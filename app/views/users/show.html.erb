<p id="notice"><%= notice %></p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<table>
  <thead>
    <tr>
      <th>Tweet</th>
      <th colspan="3"></th>
      <th>Number of Retweets</th>
      
    </tr>
  </thead>

  <tbody>
    <% @user.tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.body %></td>
        <td colspan="3"></td>
        <td><%= tweet.retweets %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
